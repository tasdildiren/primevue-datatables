{
    "name": "tasdildiren/primevue-datatables",
    "description": "Easy Laravel Server-Side implementation of PrimeVue Datatables",
    "keywords": [
        "tasdildiren",
        "primevue-datatables",
        "laravel",
        "primevue",
        "datatables",
        "server-side"
    ],
    "homepage": "https://github.com/tasdildiren/primevue-datatables",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Uğur Taşdildiren",
            "email": "tasdildiren@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": ">=8.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Tasdildiren\\PrimevueDatatables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tasdildiren\\PrimevueDatatables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tasdildiren\\PrimevueDatatables\\PrimevueDatatablesServiceProvider"
            ],
            "aliases": {
                "PrimevueDatatables": "Tasdildiren\\PrimevueDatatables\\PrimevueDatatablesFacade"
            }
        }
    }
}
